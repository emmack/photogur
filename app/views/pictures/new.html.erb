<h1> New Photo </h1>

<%= form_for @picture do |f| %>
	<p><%=f.label "artist" %><br><%=f.text_field "artist"%></p>
	<p><%=f.label "title" %><br><%=f.text_field "title"%></p>
	<p><%=f.label "url" %><br><%=f.text_field "url"%></p>
	<p><%= f.file_field "image" %></p>
	<p><%=submit_tag "Save" %></p>
<%end%>
		

