
<div class="gridwrapper">
	
			<% @pictures.each do |picture| %>
				<div class="tile">
					<div class="tileinner">
						<%= link_to image_tag(picture.url), "/pictures/#{picture.id}" %>
						<%= link_to image_tag(picture.image), "/pictures/#{picture.id}" %>
			
					</div>
				</div>
			<%end%>
</div>




